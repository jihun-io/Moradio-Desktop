{
  "name": "moradio-electron",
  "productName": "Moradio",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.119",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "concurrently": "^9.1.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "embla-carousel-react": "^8.4.0",
    "hls": "^0.0.1",
    "hls.js": "^1.5.17",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "wait-on": "^8.0.1",
    "web-vitals": "^2.1.4",
    "zustand": "^5.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build:electron": "tsc main.ts preload.ts --outDir ./build",
    "electron:serve": "npm run build:electron && concurrently -k \"cross-env BROWSER=none NODE_ENV=development npm start\" \"wait-on http://localhost:3000 && cross-env NODE_ENV=development electron .\"",
    "electron:build": "cross-env NODE_ENV=production npm run build && npm run build:electron && electron-builder",
    "electron:start": "electron ."
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "main": "build/main.js",
  "homepage": "./",
  "devDependencies": {
    "@types/hls.js": "^0.13.3",
    "tailwindcss": "^3.4.15",
    "electron": "^33.2.0",
    "electron-builder": "^25.1.8",
    "electron-is-dev": "^3.0.1"
  },
  "build": {
    "extends": null,
    "appId": "io.jihun.moradio.electron",
    "productName": "Moradio",
    "files": [
      "build/**/*"
    ],
    "directories": {
      "buildResources": "public",
      "output": "dist"
    },
    "mac": {
      "category": "public.app-category.music"
    }
  }
}
